{
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["@typescript-eslint", "prettier", "css-modules"],
  "rules": {
    "prettier/prettier": ["error"],
    "css-modules/no-unused-class": "warn",
    "css-modules/no-undef-class": "error"
  }
}
